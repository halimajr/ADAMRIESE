--configure inconsistent-assignment:assignment_sign_type:equal_sign
--configure too-many-calls-in-keyword:max_calls:50
--configure too-many-calls-in-test-case:max_calls:50
--configure too-long-test-case:max_len:60
--configure line-too-long:ignore_pattern:([\$@&]\{.+\}.*|https?://\S+)
--exclude missing-space-after-comment

--configure line-too-long:line_length:160
--configure too-many-arguments:max_args:7
--configure file-too-long:max_lines:650

--configure return_status:quality_gate:E=5:W=800:I=-1
